<%- include('includes/header.ejs',{active:'relays'}) %>
<div class="container">
    <% for (const [key,probe] of Object.entries(params.probes)) { %>
        <div class="card">
            <div class="card-header">
            <%=probe.title%> - <%=probe.desc%>
            </div>
            <div class="card-body">
            Pin <%-probe.settings().gpio%> = <%-(probe.relayState)?"<spanON</span":'OFF'%>
            </div>
        </div>
        <Br> 
    <% } %>
</div>
<%- include('includes/footer.ejs') %>


